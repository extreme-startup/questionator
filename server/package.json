{
  "name": "server",
  "version": "0.0.0",
  "description": "extreme-startup/server",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "typeorm": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:prod": "node dist/src/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "postinstall": "node ./../scripts/acknowledgements.js"
  },
  "dependencies": {
    "@nest-middlewares/cookie-parser": "^5.1.1",
    "@nest-middlewares/express-session": "^5.1.1",
    "@nestjs/common": "^5.4.0",
    "@nestjs/core": "^5.4.0",
    "@nestjs/swagger": "~3.0.2",
    "@nestjs/typeorm": "^5.3.0",
    "@nestjs/websockets": "~5.3.0",
    "@types/dotenv": "^6.1.0",
    "@types/joi": "^14.3.2",
    "connect-history-api-fallback": "^1.6.0",
    "dateformat": "^3.0.3",
    "dotenv": "^6.2.0",
    "express-mysql-session": "^2.1.0",
    "express-session": "^1.15.6",
    "handlebars": "^4.1.2",
    "integer-sequences": "^1.0.4",
    "joi": "^14.3.1",
    "json-stream-stringify": "^2.0.1",
    "mysql": "^2.16.0",
    "number-to-words": "^1.2.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.2",
    "rxjs": "^6.2.2",
    "scrabbler": "^1.1.3",
    "serve-static": "^1.13.2",
    "swagger-ui-express": "~4.0.2",
    "typeorm": "^0.2.14",
    "typeorm-store": "^1.2.0",
    "typescript": "^3.0.1",
    "uuid": "3.3.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.1.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "@types/socket.io-client": "~1.4.32",
    "@types/supertest": "^2.0.5",
    "jest": "^23.5.0",
    "jest-date-mock": "^1.0.7",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "socket.io-client": "~2.1.1",
    "supertest": "^3.1.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "collectCoverageFrom": [
      "**/*.{js,ts}",
      "!main.ts",
      "!**/*.{module,dto}.{js,ts}",
      "!{**/migration/**,**/entity/**,}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 60
      }
    },
    "testEnvironment": "node"
  }
}
